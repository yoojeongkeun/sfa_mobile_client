/*** accordion list ***/
.acclist > li {padding:0;}
.acc_head {position:relative; padding:0px 0px 28px 0px;}
.acc_head:after {
  content:""; position:absolute; right:0; top:0; width:20px; height:100%;
  background:url("../images/arrow_down.png") no-repeat left center; background-size:15px auto;
}

.acc_body {overflow:hidden;background: #bcc4d0;  height: 0px; clear: both;}
.acclist > li.on:nth-child(even) {background-color:#fff; }
.on .acc_head {background:#fff; }

.acc_head .result, .acc_head .result_on {
  float: left;
  background: #bcc4d0 url("../images/check.png")no-repeat center center;
  background-size: 15px;
  border-radius: 30px;
  width: 22px;
  height: 22px;
  margin-bottom: 0px;
  margin: 13px 6px 10px 13px;
}
.acc_head .result_on {
  background: #ff4800 url("../images/check.png")no-repeat center center;
  background-size: 15px;
}
.acc_head .tit01 {
  float: left;
  background: none;
  padding:16px 0px;
  font-size:14px; 
  color:#747474;
  border:0px solid #D9DEE5; 
  border-width:0px;
  box-shadow: none;
}
.acc_head .tit02 { 
  background: #bcc4d0 url("../images/check.png")no-repeat center center;
  background-size: 15px;
  border-radius: 30px;
  width: 6px;
  height: 6px;
  padding: 10px;
}
.on .acc_head:after {background-image:url("../images/arrow_up.png");}
.on .acc_body {clear: both;height:auto; border-top:1px solid #989898;}

