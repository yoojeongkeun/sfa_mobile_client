/* CSS Tabs */
#navlist {
        width: 95%;
        padding: 5px 0;
        margin: 10px 10px 0px 10px;
        /*border-bottom: 1px solid #778;*/
        font: bold 13px 'nanumgothic', '나눔고딕', 'malgun gothic', '맑은 고딕', 'dotum', '돋움', sans-serif;
        
}

#navlist li {
        list-style: none;
        margin: 0;
        display: inline;
}

#navlist li button {
        padding: 5px 1.5em;
        margin-left: 0px;
        border: 1px solid #bababa;
        border-bottom: none;
        background: #fff;
        text-decoration: none;
        border-radius:5px 5px 0 0;
}

#navlist li button:link { color: #448; }
#navlist li button:visited { color: #667; }

#navlist li button:hover {
        color: #4b8dcd;
        background: #fff;
        border-color: #bababa;
}

#navlist li button#current {
        background: white;        
        border: 2px solid #4b8dcd;
        border-bottom: 3px solid white;
}


