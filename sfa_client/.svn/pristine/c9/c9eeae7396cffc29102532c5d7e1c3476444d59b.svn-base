							<li class="liTab" tabindex="3"><button>결제</button></li>
                        </ul>
                        <!-- 주문자 첫번째 tab-->
                        <div class="tab1_content" id="tab1">
                        <table class="tlist00">
                        	<colgroup>
                        		<col width="*"></col>
                        		<col width="80%"></col>
                        	</colgroup>
                        	<tr>
                        		<td>성명</td>
                        		<td><input type="text" disabled style="background-color:whitesmoke;" id="custName"></td>
                        	</tr>
                        	<tr>
	                        	<td>연락처</td>
	                        	<td><input type="text" disabled style="background-color:whitesmoke;" id="phoneNum"></td>
	                        </tr>
	                        <tr>
	                        	<td>비고</td>
	                        	<td><textarea disabled style="background-color:whitesmoke;" id="custEtc"></textarea></td>
	                        </tr>
                        </table>
                        </div>
                        <!--//주문자 첫번째 Tab-->

                		<!-- 배송 두번째 tab-->
                      	<div class="tab1_content" id="tab2" style="display: none;">
                        <table class="tlist00">
                        	<colgroup>
                        		<col width="*%"></col>
                        		<col width="30%"></col>
                        		<col width="30%"></col>
                        		<col width="20%"></col>
                        	</colgroup>
                        	<tr>
                        		<td>배송구분</td>
                        		<td>
                        		<select id="selDeliType">
                        			<option value="AJ10">택배</option>
                        			<option value="AJ20">서비스시</option>
                        		</select>
                        		</td>
                        		<td>받으시는분</td>
                        		<td><input type="text" id="deliName"></td>
                        	</tr>
                        	<tr>
                        		<td>배송지 주소</td>
                        		<td><input type="text" disabled style="background-color:whitesmoke;" id="deliAddr1"></td>
                        		<td colspan="2"><input type="text" disabled style="background-color:whitesmoke;" id="deliAddr2"></td>
                        	</tr>
                        	<tr>
                        		<td style="text-align:right"><button type="button" class="btn_search" id="btnSearchDeli" style="display:none"></button></td>
                        		<td colspan="3"><input type="text" disabled style="background-color:whitesmoke;" id="deliAddr3"></td>
                        	</tr>
                        	<tr>
                        		<td>발송여부<br>(일자)</td>
                        		<td>
                        		<select id="selDeliProc">
                        		<option value="AK10">미발송</option>
                        		<option value="AK20">발송</option>
                        		<option value="AK30">배송중</option>
                        		<option value="AK40">배송 준비중</option>
                        		</select>
                        		</td>
                        		<td colspan="2"><p class="inp_cal"><input type="text" id="deliYMD" readonly/><button type="button" id="deliCal" class="btn_cal" >달력</button></p></td>
                        		
                        	</tr>
                        	<tr>
	                        	<td>연락처</td>
	                        	<td colspan="3"><input type="text" id="deliPhoneNum"></td>
	                        </tr>
	                        <tr>
	                        	<td>배송상<br>특이사항</td>
	                        	<td colspan="3"><textarea id="deliEtc"></textarea></td>
	                        </tr>
                        </table>
                       
                        </div> 
                         <!-- //배송 두번째 tab-->

                         <!-- 결제 세번째 tab-->
                         <div class="tab1_content" id="tab3" style="display: none;">
                          <table class="tlist00">
                        	<colgroup>
                        		<col width="*"></col>
                        		<col width="70%"></col>
                        	</colgroup>
                        	<tr>
                        		<td>결제구분</td>
                        		<td>
                        		<select id="selPaidType1" disabled>
                        			<option value="AH10">선청구</option>
                        			<option value="AH20">후청구</option>
                        		</select>
                        		</td>
                        	</tr>
                        	<tr>
	                        	<td>결제/수금방법</td>
	                        	<td>
	                        	<select id="selPaidType2">
	                        		<option value="3205">카드결제</option>
	                        		<option value="3220">전용계좌</option>
	                        	</select>
	                        	</td>
	                        </tr>
	                        <tr>
	                        	<td class="imgAcct">가상계좌</td>
	                        	<td><input type="text" class="imgAcct" id="accountNo" disabled></td>
	                        </tr>
	                        <tr>
	                        	<td></td>
	                        	<td style="text-align:right">
	                        		<button class="btn02" id="btnCard" style="display:none;">카드결제</button>
	                        		<button class="btn02" id="btnAdd">추가</button>
	                        		<button class="btn02" id="btnDel">삭제</button>
	                        	</td>
	                        </tr>
                        </table>
                        </div>
                         <!-- //결제 세번째 tab-->
                 </div>
            </li>
