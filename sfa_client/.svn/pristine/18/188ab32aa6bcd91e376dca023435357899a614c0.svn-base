/*** list ***/
.list01 {border-top:1px solid #D9DEE5;}
.list01 li {position:relative; padding:10px; border-bottom:1px solid #D9DEE5;}
.list01 li:nth-child(even) {background:#f8f8f8;}
.list01 li .tit {margin-bottom:5px; font-size:1.2em;}
.list01 li.cur {background:#eef2f4;}
.tit01 + .list01 {border-top:none;}

.tab01 + .list01 {border-top:none;}

.list02 {padding:3px 0;}
.list02 li {padding:2px 10px;}

/*** accordion list ***/
.acclist > li {padding:0;}
.acc_head {position:relative; padding:10px 20px 10px 10px;}
.acc_head:after {content:""; position:absolute; right:0; top:0; width:20px; height:100%;
	background:url("../images/arrow_down.png") no-repeat left center; background-size:15px auto;
}
.acc_head .tit {margin-bottom:5px; font-size:1.2em;}
.acc_body {height:0; overflow:hidden;}
.acclist > li.on:nth-child(even) {background-color:#fff;}
.on .acc_head {background:#fff3dd;}
.acc_head.tit02 {padding-left:30px; background:#fff url("../images/bul_tit02.png") no-repeat 10px center; background-size:15px auto; height: 15px; font-size: 14px;}
.on .acc_head:after {background-image:url("../images/arrow_up.png");}
.on .acc_body {height:auto; border-top:1px solid #D9DEE5;}




