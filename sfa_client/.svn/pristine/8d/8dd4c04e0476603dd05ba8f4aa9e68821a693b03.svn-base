<body>
<div class="subWrap01">
	<header class="hide">
		<h1>서비스일정</h1>
	</header>
	
	<section class="contentsWrap">
		<!-- top fix -->
		<div class="header_fix">
			<input type="text" id="datepicker1" style="display:none;" /><button type="button" class="btn_cal btn_left" id="calendar" style="top: 28px;">달력</button>
			<h2 class="tit"><span id="workdates"></span> <span id="days"></span></h2>
			<button type="button" class="btn_refresh btn_right" id="btnRefresh" style="top: 31px;">새로고침</button>
			<button type="button" class="btn_refresh btn_right" id="btnMapOnOff" style="right: 40px; top: 31px;">새로고침</button>	
			
						
			<!-- map -->
			<div class="ollehmap ollehmap-event" id="map_div" style="cursor: default; height: 250px;" data-id="ollehmap1407227630721"></div>
			<!-- map -->		
		</div>
		<!-- //top fix -->
		
		<!-- scroll -->
		<div class="contsection">

			
			
			<!-- map -->
			<!-- <div class="map" id="maplist">
				<ul class="work_stat">
					<li>
						<dl>
							<dt>전체</dt>
							<dd><strong>00</strong></dd>
						</dl>
					</li>
					<li class="end">
						<dl>
							<dt>완료</dt>
							<dd><strong>3</strong></dd>
						</dl>
					</li>
				</ul>
			</div> -->
			<!-- //map -->
			
			<!-- schedule list -->
			<ul class="list01 service_list acclist" id="servicelistNew" style="-webkit-tap-highlight-color: rgba(0, 0, 0, 0); margin-top: 248px;"></ul>
			<ul class="list01 service_list acclist" id="servicelist" style="display:none">
				<li class="listopen">	<!-- list open: "on" class 추가 -->
					<!-- accordion head -->
					<div class="acc_head btnlist" custtype="" workymd="" workcnt="" custcode="" srNum="" freeSRYN="" workmngyn="" workType ="">
						<span class="num">1</span>
						<p class="tit">&#91;<span class="custCode"></span>&#93;<span class="custName"></span></p>
						<p class="time subtxt01 txtc01"><span class="serviceTime"></span> <span class="cust_stat" id="Pos">계약</span> <span class="cust_stat dcCall" id="Dc">확정통화결과</span></p>
					</div>
					<!-- //accordion head -->
					
					<!-- accordion body -->
					<div class="acc_body">
					    <table class="tlist01">
							<colgroup>
								<col style="width:30%;" />
								<col />
							</colgroup>
							<thead>
							<tr>
								<th scope="col">항목</th>
								<th scope="col">내용</th>
							</tr>
							</thead>
							<tbody class="serviceInfo" loadyn="">
							<tr class="customerInfoDetail">
								<th scope="row" class="contentName" contentYN ="">세스코멤버스</th>
								<td><span class="contentValue">멤버스대상아님</span></td>
							</tr>
							</tbody>
						</table>
						<div class="phone" style="display:table; margin-left:auto; margin-right:auto;" phonenumber="">
                        <button type="button" class="btn_call">전화걸기</button>
                        <button type="button" class="btn_sms">문자 보내기</button>
                        <button type="button" class="btn_map">지도 보기</button>
                        </div>
                        <div class="phone" style="display:table; margin-left:auto; margin-right:auto;" phonenumber="">
                        <button type="button" class="btn02 btn_srDel">서비스제공 취소</button>
                        </div>
                        
					</div>
					<!-- //accordion body -->
				</li>
			</ul>
