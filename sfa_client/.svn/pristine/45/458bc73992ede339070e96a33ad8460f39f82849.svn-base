tab01 = 
{	 
	list : new Array,
	
    init:function(json)
	{
    	tab01.initInterface();
    	tab01.initData(json);
		tab01.initLayout();
	},
	initInterface:function()
	{
		 $(".t1_btnSearch").click(function(){
			 bizMOB.Ui.alert("TAB1에서 조회버튼");
		 });
		 
		 $("#setTypeLarge").change(function(){
			 page.setTypeMediumCombobox($("#setTypeMedi"), $("#setTypeLarge"));
		 });
		 
		 page.setTypeLargeCombobox($("#setTypeLarge"));
		 
		 $("#t01_selHQ").change(function(){
			 page.getComboBoxList("#t01_selBR", "B01", $(this).val());
		 });
		 
		 $("#t01_selBR").change(function(){
			 page.getComboBoxList("#t01_selUserID", "C01", $(this).val());
		 });
			
	},	 
	initData:function(json)
	{
		 
	},	 
	initLayout:function()
	{	
		// 본부 콤보박스
		page.getComboBoxList("#t01_selHQ", "A01", "");
	}
};


