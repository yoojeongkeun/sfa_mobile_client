				</tr>
				<tr>
					<th>
						<span>담당자</span>
					</th>
					<td style="border-bottom: 0px;">
						<select id="selUser"></select>
					</td>
				</tr>			
		</tbody>
		</table>
	</section>
	
	<footer class="popbot_btn">
		<button type="button" class="btn_02" id="btnSelect">배정</button>
