#css_tabs {
    font-family:'nanumgothic', '나눔고딕', 'malgun gothic', '맑은 고딕', 'dotum', '돋움', sans-serif;
}
/* 탭 선택 시 표시할 요소(div) 정의(1번 탭 선택 시 첫 번째 div 요소 표시) */
#css_tabs .fix input[type=radio]:nth-of-type(1), #css_tabs .fix input[type=radio]:nth-of-type(1) ~ div:nth-of-type(1), #css_tabs .fix input[type=radio]:nth-of-type(2), #css_tabs .fix input[type=radio]:nth-of-type(2) ~ div:nth-of-type(2) {
    display:none
}
#css_tabs .fix input[type=radio]:nth-of-type(1):checked ~ div:nth-of-type(1), #css_tabs .fix input[type=radio]:nth-of-type(2):checked ~ div:nth-of-type(2) {
    display:block
}
/* 라벨 기본 스타일 지정 */
#css_tabs .fix {position: fixed; width:98.3%; text-align:center; padding-top: 10px;}
#css_tabs .fix label {
    display:inline-block;
    font-variant:small-caps;
    font-size:0.9em;
    padding:3px;
    text-align:center;
    width:43%;
    line-height:1.8em;
    font-weight:bold;
    border-radius:3px 3px 0 0;
    background:#eee;
    color:#777;
    border:1px solid #ccc;
    border-bottom: 1px solid #eee;
    border-width:1px 1px 1px;
}
#css_tabs .fix label:hover {
    cursor:pointer
}


/* 실제 내용이 담긴 div 요소 스타일 지정 */
#css_tabs .tab1_content, #css_tabs .tab2_content {
    padding:3em;
    border:1px solid #ddd;    
    height:100%;
    width:90%;
    text-align:center;
}

