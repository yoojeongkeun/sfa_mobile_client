@charset "UTF-8";


/*** login ***/
.loginWrap input[type=number], .loginWrap input[type=password]
{display:inline-block; height:40px; margin:0; padding:0 5px; font:normal 1em '맑은 고딕', dotum,'돋움',sans-serif; color:#737373; vertical-align:middle;
	background:#fff; border:1px solid #dadada; border-radius:3px; box-shadow:2px 2px 3px rgba(0,0,0,0.35); -webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box; -webkit-appearance:none;
}
.loginWrap {height:50px; padding:15% 10px 10px; box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box;}
.loginWrap header {margin-bottom:40px;}
.loginWrap header h1 {height:71px; text-indent:-999em; background:url("../images/logo.png") no-repeat center top; background-size:auto 100%;}
.loginWrap .logo_sub {height:13px; margin-bottom:10px; text-indent:-999em; background:url("../images/logo_sub.png") no-repeat center top; background-size:auto 100%;}
.loginWrap fieldset {padding:25px 12px; background:#e7e7e7; border-radius:5px;}
.inp {margin-bottom:10px;}
.inp label {position:absolute; left:-999px; top:-999px; font-size:0;}
.inp input {width:100%; height:50px; padding-left:40px; font-size:1.15em;
	background-repeat:no-repeat; background-position:10px center; background-size:25px auto;
}
.id input {background-image:url("../images/ico_id.png");}
.pw input {background-image:url("../images/ico_pw.png");}
.loginWrap input[type=checkbox]:checked {background-color:#fff;}
.loginWrap .opt {margin:20px 0; font-weight:bold; font-size:1.16em; color:#484848; text-align:center;}
.btn_login {width:100%; height:50px; font-weight:bold; font-size:1.35em; color:#fff; background:#0188cc; border:1px solid #006ea6; border-radius:3px;}

/*** main ***/
.mainWrap {position:relative; min-height:100%;}
.mainWrap header {position:relative; padding:0 50px;}
.mainWrap header h1 {height:45px; text-indent:-999em; background:url("../images/logo.png") no-repeat center center; background-size:auto 80%;}
.mainWrap header button {position:absolute; top:0; width:40px; height:100%; text-indent:-999em; background-repeat:no-repeat; background-position:center center; background-size:auto 26px;}
.mainWrap header .btn_end {left:5px; background-image:url("../images/btn_end.png");}
.mainWrap header .btn_allmenu {right:5px; background-image:url("../images/btn_allmenu.png");}
.work_stat {height:240px; background:#1c3a63;}
.work_stat .sum {position:relative; padding:35px 10px 10px 35%; color:#fff; background:url("../images/bg_worksum.png") repeat-x 0 0; background-size:100% 100%;}
.pers_info {position:absolute; left:10px; top:15px; right:67%; text-align:center; letter-spacing:-1px; text-overflow:ellipsis; text-shadow:1px 1px rgba(0,0,0,0.2); white-space:nowrap; overflow:hidden;}
.date span {display:inline-block; padding:5px 12px; font-weight:bold; background:#0a1d27; border-radius:15px;}
.name {margin:6px 0; font-size:1.5em;}
.depart {font-size:0.9em;}

.sum .stat {padding-bottom:3px; border-bottom:1px solid #48a5d1; overflow:hidden;}
.stat li {float:left; position:relative; width:33%; padding:0 5px 1.3em; text-align:center;
	border-left:1px solid #349cce; border-right:1px solid #50aad5;
	box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box;
}
.stat li:first-child {border-left:none;}
.stat li:last-child {border-right:none;}
.stat dt, .stat2 dt {position:absolute; left:0; bottom:3px; width:100%; font-size:0.8em; color:#a8eeef;}
.stat dd span {display:inline-block; width:40px; height:40px; font-weight:bold; font-size:1.4em; line-height:36px;
	background:url("../images/bg_count01.png") no-repeat center center; background-size:auto 34px;
}
.stat .end dd span {background-image:url("../images/bg_count02.png");}
.stat .notend dd span {background-image:url("../images/bg_count03.png");}
.stat2 {margin-top:1px; padding-top:5px; border-top:1px solid #278dbd; overflow:hidden;}
.stat2 li {float:left; position:relative; width:33%; padding-bottom:1.3em; text-align:center;}
.stat2 dt {bottom:0;}
.stat2 dd {font-weight:bold; font-size:1.4em;}



.btn_refresh {position:absolute; right:5px; top:5px; width:30px; height:30px; text-indent:-999em;
	background:url("../../common/images/btn_refresh.png") no-repeat center center; background-size:19px auto;
}

.work_stat .sch {position:relative; padding:5px 10px 5px 35%; color:#a2bfd4; background:url("../images/bg_worksch.png") repeat-x 0 0; background-size:100% 100%;}
.sch .tit {position:absolute; left:10px; top:50%; right:70%; margin-top:-26px; padding-top:35px; font-size:0.9em; color:#6d93aa; text-align:center;
	background:url("../images/ico_sch_tit.png") no-repeat center top; background-size:auto 28px;
}
.slide_list {padding:10px 40px 10px 0;}
.slide_list ul {height:3.2em; overflow:hidden;}
.slide_list li {height:100%;}
.slide_list .tt {margin-bottom:7px; font-size:1.4em; letter-spacing:-0.05em; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.slide_list .time {font-size:0.9em; color:#3a678a;}
.btn_up, .btn_down {position:absolute; right:10px; width:30px; height:30px; text-indent:-999em; background-repeat:no-repeat; background-position:center center; background-size:13px auto;}
.btn_up {top:10px; background-image:url("../images/btn_up.png");}
.btn_down {bottom:10px; background-image:url("../images/btn_down.png");}

/* main menu */
.main_menu {position:absolute; left:0; top:285px; right:0; bottom:0;}
.main_menu nav {height:100%;}
.main_menu ul {padding:10px; overflow:hidden; box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box;}
.main_menu ul.group01 {height:32%; background:#d3e7f7;}
.main_menu ul.group02 {height:100%;}
.main_menu li {float:left; position:relative; width:33%; height:50%; text-align:center;
	border:1px solid #ddd; border-color:#ddd #eee #eee #ddd;
	box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box;
}
.main_menu ul.group01 li {width:25%; height:100%; border-width:0 1px 0 0; border-color:#bdccd8;}
.main_menu ul.group01 li:last-child {border-right:none;}

.main_menu .group02 li:after {content:""; position:absolute; right:-5px; bottom:-5px; width:8px; height:8px; background:#fff; z-index:100;}
.main_menu li:nth-child(3n):after {display:none;}
.main_menu .group02 li:nth-child(3n-1) {width:34%;}
.main_menu .group02 li:nth-child(3n+1) {border-left:none;}
.main_menu .group02 li:nth-child(3n) {border-right:none;}
.main_menu li:nth-child(1) {border-top:none;}
.main_menu li:nth-child(2) {border-top:none;}
.main_menu li:nth-child(3) {border-top:none;}
.main_menu li:nth-child(4) {border-bottom:none;}
.main_menu li:nth-child(5) {border-bottom:none;}
.main_menu li:nth-child(6) {border-bottom:none;}
.main_menu button {width:100%; height:100%; color:#707070;
	background-repeat:no-repeat; background-position:center 30%; background-size:40px auto;
}
.main_menu button span {position:absolute; left:0; bottom:12px; width:100%;}
.main_menu .group01 button {background-position:center 20%;}
.main_menu .group01 button span {bottom:5px;}
.main_menu .km {background-image:url("../images/menu_km.png");}
.main_menu .km1 {background-image:url("../images/menu_km_1.png");}
.main_menu .start {background-image:url("../images/menu_start.png");}
.main_menu .arrival {background-image:url("../images/menu_arrival.png");}
.main_menu .start1 {background-image:url("../images/menu_start_1.png");}
.main_menu .arrival1 {background-image:url("../images/menu_arrival_1.png");}
.main_menu .visitSch {background-image:url("../images/menu_route.png");}
.main_menu .totalSearch {background-image:url("../images/menu_totalSearch.png");}
.main_menu .salesActivity {background-image:url("../images/menu_salesActivity.png");}
.main_menu .salesPlan {background-image:url("../images/menu_salesPlan.png");}



.main_menu .info {background-image:url("../images/menu_info.png");}
.main_menu .div {background-image:url("../images/menu_div.png");}
.main_menu .card {background-image:url("../images/menu_card.png");}
.main_menu .card_cancel {background-image:url("../images/menu_cardcancel.png");}
.main_menu .refuel {background-image:url("../images/menu_refuel.png");}
.main_menu .collect {background-image:url("../images/menu_collect.png");}
.main_menu .plan {background-image:url("../images/menu_sch.png");}
.main_menu .end {background-image:url("../images/menu_end.png");}
.main_menu .service_info {background-image:url("../images/menu_serviceinfo.png");}
.main_menu .cs {background-image:url("../images/menu_cs.png");}
.main_menu .customerMove {background-image:url("../images/menu_cs.png");}

/*** 전체메뉴 ***/
.menuWrap {position:relative; min-height:100%; padding:18px 50px 18px 18px; background:#fff; box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box;}
.menuWrap .tit {padding:10px 5px; font-size:1.05em; color:#176e9b; background:#e4e4e4;}
.menuWrap .tit .welcome {display:inline;}
.btn_logout {margin-left:5px; padding:3px 7px; font-size:1.05em; color:#fff; vertical-align:middle; background:#f3723f; border:1px solid #d56336; border-radius:3px; box-shadow:1px 2px 3px rgba(0,0,0,0.4);}
.menuWrap nav ul {border-bottom:1px solid #d1d1d1;}
.menuWrap nav li {border-top:1px solid #d1d1d1; border-bottom:1px solid #efefef;}
.menuWrap nav button {display:block; width:100%; padding:20px 10px; font-size:1.05em; color:#686868; text-align:left;}
.menuWrap .btn_close {position:absolute; right:5px; top:10px; width:40px; height:30px; text-indent:-999em;
	background:url("../images/btn_close.png") no-repeat center center; background-size:16px auto;
}

/*** 시작 Km 등록 ***/
.km {height:100%;}
.km .contentsWrap {height:100%;}
.km .table_fix {position:absolute; left:0; top:205px; right:0; bottom:56px; min-height:100px;}


.bg02{background-color: #ffddaa;}

